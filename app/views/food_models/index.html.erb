<div class="row">
  <div class="col-md-12">
    <%= panel_for @food_models do %>
      <% if @food_models.empty? %>
        <%= alert_empty @food_models %>
      <% else %>
        <div class="table-responsive">
          <table id="data-table" class="table table-striped table-bordered">
            <thead>
              <tr>
                <th>Brand</th>
                <th>Name</th>
              </tr>
            </thead>
            <tbody>
            <% @food_models.each do |model| %>
              <tr>
                <td><%= model.brand %></td>
                <td><%= model.name %></td>
              </tr>
            <% end %>
            </tbody>
          </table>
        </div>
      <% end %>
      <%= link_to 'New Food model', new_food_model_path, class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
