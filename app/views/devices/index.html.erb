<div class="row">
  <div class="col-md-12">
    <%= panel_for @devices do %>
      <div class="table-responsive">
        <table id="data-table" class="table table-striped table-bordered">
          <thead>
            <tr>
              <th>Name</th>
            </tr>
          </thead>
          <tbody>
          <% @devices.each do |model| %>
            <tr>
              <td><%= link_to model.name, battery_pack_type_path(model) %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
      <hr>
      <%= link_to 'New devices', new_device_path, class: 'btn btn-primary m-r-5 m-b-5' %>
    <% end %>
  </div>
</div>
