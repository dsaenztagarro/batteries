<%= form_for device do |f| %>
	<%= f.text_field_group :name, disabled: @disabled %>

  <fieldset>
    <legend>Properties</legend>
    <ul id="device-property-list">
    <%= f.fields_for :properties do |builder| %>
      <%= render 'property_fields', f: builder %>
    <% end %>
    </ul>
  </fiedset>

	<%= f.submit %>
<% end %>
