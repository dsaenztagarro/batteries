<div class="row">
  <div class="col-md-12">
    <%%= panel_for @<%= plural_table_name %> do %>
      <%% if @<%= plural_table_name %>.empty? %>
        <%%= alert_empty @<%= plural_table_name %> %>
      <%% else %>
        <div class="table-responsive">
          <table id="data-table" class="table table-striped table-bordered">
            <thead>
              <tr>
<% attributes.reject(&:password_digest?).each do |attribute| -%>
                <th><%= attribute.human_name %></th>
<% end -%>
              </tr>
            </thead>
            <tbody>
            <%% @<%= plural_table_name %>.each do |model| %>
              <%% cache model do %>
                <tr>
<% attributes.reject(&:password_digest?).each do |attribute| -%>
                  <td><%%= model.<%= attribute.name %> %></td>
<% end -%>
                </tr>
              <%% end %>
            <%% end %>
            </tbody>
          </table>
        </div>
      <%% end %>
      <%%= link_to 'New <%= human_name %>', new_<%= singular_table_name %>_path, class: 'btn btn-primary' %>
    <%% end %>
  </div>
</div>
